
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script type="text/javascript">

    $(document).ready(function () {
        $("#divContentLeft").load('@Url.Action("Navigator", "UserType", new { area = "Security" })?userTypeId=@ViewData["UserTypeId"]');
        $("#divContentRight").load('@Url.Action("Summary", "UserType", new { area = "Security" })?userTypeId=@ViewData["UserTypeId"]');
    });

</script>

<div class="row">
    <div class="col-lg-12">
        <h1 style="margin: 0px 0px 15px 15px;">Tipo de Usuarios</h1>
    </div>
</div>

<div class="col-md-6" >
    <div class="box box-default">
        <div id="divContentLeft"></div>
    </div>
</div>
<div class="col-md-6">
    <div class="box box-default">
        <div id="divContentRight"></div>
    </div>
</div>
